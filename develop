//                                    ordOrdenSol.setFechaCumplimiento(calendar.getTime());
//                                    ordOrdenSol.setFechaTranscripcion(calendar.getTime());
//                                    ordOrdenSol.setFechaApliacacion(calendar.getTime());
//                                    ordOrdenSol.setFechaActualizacion(calendar.getTime());
